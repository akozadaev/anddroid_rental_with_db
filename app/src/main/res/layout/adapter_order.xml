<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <TextView android:id="@+id/textId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="id заказа"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView android:id="@+id/textMoney"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Стоимость заказа"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView android:id="@+id/textStartdate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Дата заказа"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView android:id="@+id/textTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Время аренды"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView android:id="@+id/textIdClient"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="id клиента"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView android:id="@+id/textIdInstruments"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="id инструментов"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView android:id="@+id/textStatus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Статус заказа"
        android:textAppearance="?android:attr/textAppearanceMedium" />

</LinearLayout>